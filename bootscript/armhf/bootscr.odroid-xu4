setenv condev "console=ttySAC2,115200n8 console=tty0 quiet"
setenv bootdev "/dev/mmcblk${devnum}p1"
setenv bootargs "root=${bootdev} rootwait ro ${condev}"

@@UBOOT_ENV_EXTRA@@

@@UBOOT_PREBOOT_EXTRA@@

load ${devtype} 0:1 ${ramdisk_addr_r} /install/uInitrd
load ${devtype} 0:1 ${kernel_addr_r} /install/zImage
load ${devtype} 0:1 ${fdt_addr_r} /install/dtbs/current/exynos/${fdtfile}
fdt addr ${fdt_addr_r}

bootz ${kernel_addr_r} ${ramdisk_addr_r} ${fdt_addr_r}
